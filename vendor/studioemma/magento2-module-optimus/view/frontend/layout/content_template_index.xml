<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>
            Boilerplate with all possible html tags - for development purposes
        </title>
    </head>
    <body>
        <!-- NORMAL HEAD TITLE IS NOT TRANSLATED -->
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Boilerplate HTML</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" template="StudioEmma_Optimus::content/template.phtml" name="optimus-content-html" />
        </referenceContainer>
    </body>
</page>
